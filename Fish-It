        gui.rareToggle.BackgroundColor3 = CONFIG.RARE_FISH_ONLY and Color3.fromRGB(255, 165, 0) or Color3.fromRGB(70, 130, 180)
    end)
    
    gui.closeBtn.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)
    
    gui.minBtn.MouseButton1Click:Connect(function()
        local isMinimized = gui.mainFrame.Size.Y.Offset <= 80
        
        if isMinimized then
            -- Restore window
            local tween = TweenService:Create(gui.mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
                Size = UDim2.new(0, 420, 0, 350)
            })
            tween:Play()
            gui.minBtn.Text = "âˆ’"
        else
            -- Minimize window
            local tween = TweenService:Create(gui.mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
                Size = UDim2.new(0, 420, 0, 60)
            })
            tween:Play()
            gui.minBtn.Text = "â–¡"
        end
    end)
    
    -- Dragging functionality
    local dragging = false
    local dragStart = nil
    local startPos = nil
    
    local function startDrag(input)
        dragging = true
        dragStart = input.Position
        startPos = gui.mainFrame.Position
    end
    
    local function updateDrag(input)
        if dragging and dragStart then
            local delta = input.Position - dragStart
            local newPos = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, 
                                   startPos.Y.Scale, startPos.Y.Offset + delta.Y)
            gui.mainFrame.Position = newPos
            gui.shadow.Position = UDim2.new(0, newPos.X.Offset - 7, 0, newPos.Y.Offset - 7)
        end
    end
    
    local function endDrag()
        dragging = false
    end
    
    -- Connect drag events to title bar
    gui.mainFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            startDrag(input)
        end
    end)
    
    gui.mainFrame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            updateDrag(input)
        end
    end)
    
    gui.mainFrame.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            endDrag()
        end
    end)
    
    -- Entrance animation
    gui.mainFrame.Size = UDim2.new(0, 0, 0, 0)
    gui.mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    gui.shadow.BackgroundTransparency = 1
    
    local entranceTween = TweenService:Create(gui.mainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 420, 0, 350),
        Position = UDim2.new(0.5, -210, 0.5, -175)
    })
    
    local shadowTween = TweenService:Create(gui.shadow, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
        BackgroundTransparency = 0.7
    })
    
    entranceTween:Play()
    shadowTween:Play()
    
    -- Add glow effect on hover for buttons
    local function addHoverEffect(button, hoverColor, originalColor)
        button.MouseEnter:Connect(function()
            local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor})
            tween:Play()
        end)
        
        button.MouseLeave:Connect(function()
            local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = originalColor})
            tween:Play()
        end)
    end
    
    addHoverEffect(gui.toggleBtn, Color3.fromRGB(255, 120, 120), CONFIG.ENABLED and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(100, 255, 100))
    addHoverEffect(gui.testBtn, Color3.fromRGB(120, 170, 255), Color3.fromRGB(100, 150, 255))
    addHoverEffect(gui.rareToggle, Color3.fromRGB(255, 185, 20), CONFIG.RARE_FISH_ONLY and Color3.fromRGB(255, 165, 0) or Color3.fromRGB(70, 130, 180))
    addHoverEffect(gui.closeBtn, Color3.fromRGB(240, 70, 70), Color3.fromRGB(220, 50, 50))
    addHoverEffect(gui.minBtn, Color3.fromRGB(255, 213, 27), Color3.fromRGB(255, 193, 7))
end

-- Setup fish detection
setupFishDetection()

-- Auto-detect webhook URL from clipboard (if possible)
local function tryAutoDetectWebhook()
    -- This would require additional permissions, so just show instruction
    if CONFIG.WEBHOOK_URL == "" then
        StarterGui:SetCore("SendNotification", {
            Title = "âš ï¸ Webhook Setup Required";
            Text = "Edit script dan masukkan Discord Webhook URL!";
            Duration = 8;
        })
    end
end

